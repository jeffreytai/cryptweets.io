<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.crypto.entity.CoinSentiment" table="CoinSentiment">
        <id name="Id" type="java.lang.Integer">
            <column name="Id" />
            <generator class="identity" />
        </id>
        <property name="symbol" type="string">
            <column name="Symbol" length="50" not-null="true" />
        </property>
        <property name="twitterChange_24h" type="java.math.BigDecimal">
            <column name="TwitterChange_24h" not-null="false" />
        </property>
        <property name="twitterVolume_24h" type="java.lang.Long">
            <column name="TwitterVolume_24h" not-null="false" />
        </property>
        <property name="redditChange_24h" type="java.math.BigDecimal">
            <column name="RedditChange_24h" not-null="false" />
        </property>
        <property name="redditVolume_24h" type="java.lang.Long">
            <column name="RedditVolume_24h" not-null="false" />
        </property>
        <property name="socialVolumeChange_24h" type="java.math.BigDecimal">
            <column name="SocialVolumeChange_24h" not-null="false" />
        </property>
        <property name="socialVolume_24h" type="java.lang.Long">
            <column name="SocialVolume_24h" not-null="false" />
        </property>
        <property name="timestamp" type="java.lang.Long">
            <column name="Timestamp" not-null="false" />
        </property>
        <property name="batchNum" type="java.lang.Integer">
            <column name="BatchNum" not-null="true" />
        </property>
    </class>
</hibernate-mapping>